{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
    { "src": "api/trpc/[trpc].ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/trpc/(.*)", "dest": "/api/trpc/[trpc].ts" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}


